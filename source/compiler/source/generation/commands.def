#
#		Routines do all the work
#
GO 				X:CommandGOAlt N
GOTO 			X:CommandGOTO N
GOSUB 			X:CommandGOSUB N
LET 			X:CommandLET N
INPUT 			X:CommandINPUT N
INPUT# 			C:CommandINPUTStream N
PRINT 			X:CommandPRINT N
PRINT# 			C:CommandPRINT N
REM 			X:CommandREM N
FOR 			X:CommandFOR N
NEXT 			X:CommandNEXT N
GET 			X:CommandGET N
DATA 			X:CommandDATA N
READ 			X:CommandREAD N
DIM 			X:CommandDIM N
DEF 			X:CommandDEF N
#
#		Routine supported
#
IF 				# X:CommandIF N
WAIT 			#,# X:CommandWAIT N
ON 				# T X:CommandON N
#
#		Controls
#
SYS 			# T N
POKE 			#,# T N
#
#		Token only
#
RETURN 			T N
STOP 			T N
END 			T N
RESTORE 		T N

